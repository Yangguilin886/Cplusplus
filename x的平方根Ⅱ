class Solution {
public:
    int mySqrt(int x) {
        int left=0,right=sqrt(INT_MAX);
        while(left<right)
        {
            int mid=left+(right-left+1)/2;
            if(mid*mid<=x)
            {
                left=mid;
            }
            else
            {
                right=mid-1;
            }
        }
        return left;
    }
};