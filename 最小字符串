#include<iostream>
#include<vector>
#include<string>
#include<algorithm>
using namespace std;
int main()
{
    int n,m;
    cin>>n>>m;
    string s;
    vector<char> ret(m);
    cin>>s;
    for(int i=0;i<m;i++)
    {
        cin>>ret[i];
    }
    sort(ret.begin(),ret.end());
    int i=0,j=0;
    while(i<s.size())
    {
        if(j==ret.size())break;
        if(ret[j]<s[i])
        {
            s.insert(s.begin()+i,ret[j]);
            j++;
        }
        else
        {
            if(i==s.size()-1)
            {
                s+=ret[j];
                j++;
            }
            i++;
        }
    }
    cout<<s<<endl;
    return 0;
}