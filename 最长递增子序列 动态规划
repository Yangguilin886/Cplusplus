class Solution {
public:
    int lengthOfLIS(vector<int>& nums) {
        int dp[2501]={0};
        dp[0]=1;
        int max_i=1;
        for(int i=1;i<nums.size();i++)
        {
            int n=0;
            for(int j=0;j<i;j++)
            {
                if(nums[i]>nums[j])
                {
                    n=max(n,dp[j]);
                }
            }
            dp[i]=n+1;
            max_i=max(max_i,dp[i]);
        }
        return max_i;
    }
};