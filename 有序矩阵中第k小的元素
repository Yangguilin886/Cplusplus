class Solution {
public:
    int kthSmallest(vector<vector<int>>& matrix, int k) {
        vector<int> ret;
        for(size_t i=0;i<matrix.size();i++)
        {
            for(size_t j=0;j<matrix[i].size();j++)
            {
                ret.push_back(matrix[i][j]);
            }
        }
        sort(ret.begin(),ret.end());
        return ret[k-1];
    }
};