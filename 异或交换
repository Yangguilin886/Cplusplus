#include<iostream>
#include<vector>
#include<string>
using namespace std;
int main()
{
    long long n=0,t=0;
    cin>>n>>t;
    int x=1;
    while(x<n)
    {
        x<<=1;
    }
    t=t%x;
    string s;
    cin>>s;
    for(int i=0;i<t;i++)
    {
        for(int j=n-1;j>=1;j--)
        {
            s[j]=(s[j]-'0')^(s[j-1]-'0')+'0';
        }
    }
    cout<<s<<endl;
    return 0;
}