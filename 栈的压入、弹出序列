class Solution {
  public:
    bool IsPopOrder(vector<int>& pushV, vector<int>& popV) {
        stack<int> Push;
        size_t flag = 0;
        for (size_t i = 0; i < pushV.size(); i++) {
            Push.push(pushV[i]);
            while(!Push.empty()&&flag<popV.size()&&Push.top()==popV[flag])
            {
                Push.pop();
                flag++;
            }
        }
        if (Push.empty()) {
            return true;
        }
        return false;
    }
};