class Solution {
public:
    int minCostClimbingStairs(vector<int>& cost) {
        int dp[1000]={0};
        dp[0]=cost[0];
        dp[1]=cost[1];
        for(int i=2;i<cost.size();i++)
        {
            int n=dp[i-1]+cost[i];
            int m=dp[i-2]+cost[i];
            dp[i]=min(n,m);
        }
        int n=cost.size();
        return min(dp[n-1],dp[n-2]);
    }
};