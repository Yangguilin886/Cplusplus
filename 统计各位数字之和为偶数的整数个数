class Solution {
public:
    int countEven(int num) {
        int n=0;
        for(int i=2;i<=num;i++)
        {
            int m=i;
            int flag=0;
            while(m>0)
            {
                flag+=m%10;
                m/=10;
            }
            if(flag%2==0)
            {
                n++;
            }
        }
        return n;
    }
};