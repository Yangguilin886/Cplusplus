class Solution {
public:
    int removeDuplicates(vector<int>& nums) {
        int ret[20001]={0};
        int left=0,right=0;
        while(right<nums.size())
        {
            if(ret[nums[right]+10000]<2)
            {
                nums[left++]=nums[right];
                ret[nums[right]+10000]++;
            }
            right++;
        }
        return left;
    }
};