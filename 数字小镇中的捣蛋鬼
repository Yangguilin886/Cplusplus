class Solution {
public:
    vector<int> getSneakyNumbers(vector<int>& nums) {
        unordered_map<int,int> hash;
        for(int i=0;i<nums.size();i++)
        {
            hash[nums[i]]++;
        }
        vector<int> ret;
        for(auto [x,y]:hash)
        {
            if(y>=2)
            {
                ret.push_back(x);
            }
        }
        return ret;
    }
};