class Solution {
public:
    bool hasSameDigits(string s) {
        while (s.size() > 2) {
            string ret;
            for (int i = 0; i < s.size() - 1; i++) {
                int n = (s[i] - '0') + (s[i + 1] - '0');
                n %= 10;
                ret += to_string(n);
            }
            s=ret;
        }
        if(s[0]==s[1])
        {
            return true;
        }
        return false;
    }
};