class Solution {
public:
    int maxProfit(vector<int>& nums) {
        int dp[5001][3]={0};
        dp[0][0]=-nums[0];
        for(int i=1;i<nums.size();i++)
        {
            dp[i][0]=max(dp[i-1][0],dp[i-1][1]-nums[i]);
            dp[i][1]=max(dp[i-1][1],dp[i-1][2]);
            dp[i][2]=dp[i-1][0]+nums[i];
        }
        return max(dp[nums.size()-1][1],dp[nums.size()-1][2]);
    }
};