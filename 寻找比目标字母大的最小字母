class Solution {
public:
    char nextGreatestLetter(vector<char>& letters, char target) {
        char flag='z';
        int n=0;
        for(auto e: letters)
        {
            if(e>target&&e<=flag)
            {
                flag=e;
                n=1;
            }
        }
        if(n==0)
        {
            return letters[0];
        }
        return flag;
    }
};