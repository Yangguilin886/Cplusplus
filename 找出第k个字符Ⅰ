class Solution {
public:
    char kthCharacter(int k) {
        string ret;
        ret+="a";
        string s;
        while(ret.size()<k)
        {
            s=ret;
            for(auto e:ret)
            {
                s+=(e+1);
            }
            ret=s;
        }
        return ret[k-1];
    }
};