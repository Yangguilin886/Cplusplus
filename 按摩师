class Solution {
public:
    int massage(vector<int>& nums) {
        if(nums.size()==0)return 0;
        int f[1000]={0};
        int g[1000]={0};
        f[0]=nums[0];
        for(int i=1;i<nums.size();i++)
        {
            f[i]=g[i-1]+nums[i];
            g[i]=max(g[i-1],f[i-1]);
        }
        return max(f[nums.size()-1],g[nums.size()-1]);
    }
};