class Solution {
public:
    int change(int amount, vector<int>& coins) {
    int n=coins.size();
    vector<long long> dp(amount+1);
    dp[0]=1;
    for(int i=0;i<n;i++)
    {
        for(int j=coins[i];j<amount+1;j++)
        {
            dp[j]+=dp[j-coins[i]];
            dp[j]%=INT_MAX;
        }
    }
    return dp[amount]%INT_MAX;
}
};