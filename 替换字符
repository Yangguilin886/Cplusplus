#include<iostream>
#include<string>
using namespace std;
int main()
{
    string s;
    cin>>s;
    int m=0;
    cin>>m;
    for(int i=0;i<m;i++)
    {
        int li=0,ri=0;
        cin>>li>>ri;
        char xi,yi;
        cin>>xi>>yi;
        int left=li-1,right=ri-1;
        while(left<=right)
        {
            if(s[left]==xi)
            {
                s[left]=yi;
            }
            if(s[right]==xi)
            {
                s[right]=yi;
            }
            left++;
            right--;
        }
    }
    cout<<s<<endl;
    return 0;
}