class Solution {
public:
    int countTestedDevices(vector<int>& nums) {
        int n=0;
        for(int i=0;i<nums.size();i++)
        {
            if(nums[i]>0)
            {
                for(int j=i+1;j<nums.size();j++)
                {
                    nums[j]=max(0,nums[j]-1);
                }
                n++;
            }
        }
        return n;
    }
};