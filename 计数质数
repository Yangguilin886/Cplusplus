class Solution {
public:
    int countPrimes(int n) {
        vector<int> ret(n, 1);
        int flag = 0;
        for (size_t i = 2; i < n; i++) {
            if (ret[i] == 1) {
                flag++;
                if (i * i < n) {
                    for (int j = i * i; j < n; j += i) {
                        ret[j] = 0;
                    }
                }
            }
        }
        return flag;
    }
};