class Solution {
    unordered_map<int,int> ret;
    int n=0;
public:
    void dfs(TreeNode* root)
    {
        if(root==nullptr)return ;
        if(!ret.count(root->val))
        {
            n++;
            ret[root->val]++;
        }
        dfs(root->left);
        dfs(root->right);
    }
    int numColor(TreeNode* root) {
        dfs(root);
        return n;
    }
};