#include<iostream>
using namespace std;
bool Goodsum(int n)
{
    int flag=1;
    while(n>0)
    {
        if(flag%2)
        {
            int m=n%10;
            if(m%2==0)
            {
                return false;
            }
        }
        else
        {
            int m=n%10;
            if(m%2)
            {
                return false;
            }
        }
        n/=10;
        flag++;
    }
    return true;
}
int main()
{
    int n=0;
    cin>>n;
    int ret=0;
    for(int i=1;i<=n;i++)
    {
        if(Goodsum(i))
        {
            ret++;
        }
    }
    cout<<ret<<endl;
    return 0;
}