class Solution {
public:
    bool canAliceWin(int n) {
        if(n<10)
        {
            return false;
        }
        int k=10,flag=0;
        while(n>=k)
        {
            n-=k;
            k--;
            flag++;
        }
        if(flag%2)
        {
            return true;
        }
        return false;
    }
};