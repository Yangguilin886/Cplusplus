class Solution {
public:
    int maxSubArray(vector<int>& nums) {
        int max_i=nums[0];
        int dp[100001]={0};
        dp[0]=nums[0];
        for(int i=1;i<nums.size();i++)
        {
            dp[i]=max(nums[i],nums[i]+dp[i-1]);
            max_i=max(dp[i],max_i);
        }
        return max_i;
    }
};