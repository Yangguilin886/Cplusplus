class Solution {
public:
    vector<int> countBits(int n) {
        vector<int> ret(n+1);
        for(int i=0;i<=n;i++)
        {
            int m=i,flag=0;
            while(m>0)
            {
                flag++;
                m&=(m-1);
            }
            ret[i]=flag;
        }
        return ret;
    }
};