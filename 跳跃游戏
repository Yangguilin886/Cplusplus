class Solution {
public:
    bool canJump(vector<int>& nums) {
        int n=nums.size();
        vector<int> dp(n);
        dp[0]=1;
        for(int i=0;i<n;i++)
        {
            for(int j=1;j<=nums[i]&&i+j<n;j++)
            {
                dp[i+j]++;
            }
            if(dp[i]==0)return false;
        }
        return true;
    }
};