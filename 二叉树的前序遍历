class Solution {
    vector<int> ret;
public:
    void dfs(TreeNode* root)
    {
        if(root==nullptr)return ;
        ret.push_back(root->val);
        dfs(root->left);
        dfs(root->right);
    }
    vector<int> preorderTraversal(TreeNode* root) {
        dfs(root);
        return ret;
    }
};