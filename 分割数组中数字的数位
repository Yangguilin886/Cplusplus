class Solution {
public:
    vector<int> separateDigits(vector<int>& nums) {
        vector<int> ret;
        for(int i=0;i<nums.size();i++)
        {
            int n=nums[i];
            vector<int> tmp;
            while(n>0)
            {
                tmp.push_back(n%10);
                n/=10;
            }
            for(int j=tmp.size()-1;j>=0;j--)
            {
                ret.push_back(tmp[j]);
            }
        }
        return ret;
    }
};