#include<iostream>
#include<vector>
using namespace std;
int main()
{
    vector<int> ret(5);
    for(int i=0;i<ret.size();i++)
    {
        cin>>ret[i];
    }
    int sum=0;
    for(int i=0;i<ret.size();i++)
    {
        int n1=ret[i]/1000;
        int n2=ret[i]%1000/100;
        int n3=ret[i]%100/10;
        int n4=ret[i]%10;
        if(n1==n3&&n4==n2+1)
        {
            sum++;
        }
    }
    cout<<sum<<endl;
    return 0;
}