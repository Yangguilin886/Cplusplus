#include <iostream>
#include<vector>
using namespace std;

int main() {
    int n,V;
    cin>>n>>V;
    vector<vector<int>> nums;
    nums.push_back({0,0});
    vector<vector<int>> dp(n+1,vector<int>(V+1));
    vector<vector<int>> ret(n+1,vector<int>(V+1));
    for(int i=1;i<V+1;i++)
    {
        ret[0][i]=-1;
    }
    for(int i=0;i<n;i++)
    {
        int a,b;
        cin>>a>>b;
        nums.push_back({a,b});
    }
    for(int i=1;i<n+1;i++)
    {
        for(int j=1;j<=V;j++)
        {
            dp[i][j]=dp[i-1][j];
            ret[i][j]=ret[i-1][j];
            if(j-nums[i][0]>=0)
            {
                dp[i][j]=max(dp[i][j],dp[i-1][j-nums[i][0]]+nums[i][1]);
                if(ret[i-1][j-nums[i][0]]!=-1)
                {
                    ret[i][j]=max(ret[i][j],ret[i-1][j-nums[i][0]]+nums[i][1]);
                }
            }
        }
    }
    cout<<dp[n][V]<<endl;
    if(ret[n][V]==-1)cout<<0<<endl;
    else
    cout<<ret[n][V]<<endl;
    return 0;
}