class Solution {
public:
    int maxArea(vector<int>& height) {
      int left=0,right=height.size()-1;
      int max=0;
      while(left<right)
      {
        int n=(right-left)*min(height[left],height[right]);
        if(n>max)
        {
            max=n;
        }
        if(height[left]<=height[right])
        {
            left++;
        }
        else
        {
            right--;
        }
      }  
      return max;
    }
};