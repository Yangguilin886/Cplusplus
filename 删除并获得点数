class Solution {
public:
    int deleteAndEarn(vector<int>& nums) {
        int n=0;
        int dp[10001]={0};
        for(int i=0;i<nums.size();i++)
        {
            dp[nums[i]]++;
            n=max(nums[i],n);
        }
        int f[10001]={0};
        int g[10001]={0};
        f[1]=dp[1];
        for(int i=2;i<=n;i++)
        {
            f[i]=g[i-1]+dp[i]*i;
            g[i]=max(f[i-1],g[i-1]);
        }
        return max(f[n],g[n]);
    }
};