class Solution {
public:
    vector<int> findDisappearedNumbers(vector<int>& nums) {
        vector<int> ret(nums.size()+1,0);
        vector<int>::iterator it=nums.begin();
        while(it!=nums.end())
        {
            ret[*it]++;
            ++it;
        }
        vector<int> tmp;
        for(size_t i=1;i<=nums.size();i++)
        {
            if(ret[i]==0)
            {
                tmp.push_back(i);
            }
        }
        return tmp;
    }
};