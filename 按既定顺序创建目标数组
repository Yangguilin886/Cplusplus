class Solution {
public:
    vector<int> createTargetArray(vector<int>& nums, vector<int>& index) {
        int n=nums.size();
        vector<int> ret;
        for(int i=0;i<n;i++)
        {
            int m=index[i];
            ret.insert(ret.begin()+m,nums[i]);
        }
        return ret;
    }
};