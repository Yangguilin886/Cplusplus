#include<iostream>
using namespace std;
int main()
{
    long long a=0,b=0,n=0;
    cin>>a>>b>>n;
    long long s=a*5+b*2;
    long long s1=n%s;
    long long s2=n/s;
    long long i=s2*7;
    if(s1>a*5)
    {
        i+=5;
        s1-=5*a;
        if(s1>b)
        {
            i+=2;
        }
        else
        {
            i+=1;
        }
    }
    else
    {
        i+=s1/a;
        if(s1%a)
        {
            i++;
        }
    }
    cout<<i<<endl;
    return 0;
}