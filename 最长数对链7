class Solution {
public:
    int findLongestChain(vector<vector<int>>& pairs) {
    sort(pairs.begin(),pairs.end());
    int dp[1001]={0};
    dp[0]=1;
    int max_i=1;
    for(int i=1;i<pairs.size();i++)
    {
        int n=0;
        for(int j=0;j<i;j++)
        {
            if(pairs[i][0]>pairs[j][1])n=max(n,dp[j]);
        }
        dp[i]=n+1;
        max_i=max(max_i,dp[i]);
    }
    return max_i;
}
};