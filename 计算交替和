class Solution {
public:
    int alternatingSum(vector<int>& nums) {
        int n=0;
        for(int i=0;i<nums.size();i++)
        {
            if(i%2==0)
            {
                n+=nums[i];
            }
            else
            {
                n-=nums[i];
            }
        }
        return n;
    }
};