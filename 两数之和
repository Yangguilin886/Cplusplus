class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {
        unordered_map<int,int> hash;
        vector<int> ret;
        for(int i=0;i<nums.size();i++)
        {
            if(hash.count(target-nums[i]))
            {
                ret.push_back(i);
                ret.push_back(hash[target-nums[i]]);
                break;
            }
            hash[nums[i]]=i;
        }
        return ret;
    }
};