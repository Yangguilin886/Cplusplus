#include <iostream>
#include <vector>
using namespace std;
void text_vector1()
{
	vector<int> v1;
	vector<int> v2(10, 1);
	vector<int> v3(++v2.begin(), --v2.end());

	for (int i = 0; i < v2.size(); i++)
	{
		cout << v2[i] << ' ';
	}
	cout << endl;

	vector<int>::iterator it = v2.begin();
	while(it != v2.end())
	{
		cout << *it << ' ';
		it++;
	}
	cout << endl;

	for (auto ch : v2)
	{
		cout << ch << ' ';
	}
	cout << endl;
}
void text_vector2()
{
	vector<int> v1(10, 1);

	v1.reserve(20);
	cout << v1.size() << endl;
	cout << v1.capacity() << endl;

	v1.reserve(15);
	cout << v1.size() << endl;
	cout << v1.capacity() << endl;

	v1.reserve(5);
	cout << v1.size() << endl;
	cout << v1.capacity() << endl;
}
void text_vector3()
{
	vector<int> v1(10, 1);
	v1.resize(15, 2);
	cout << v1.size() << endl;
	cout << v1.capacity() << endl;

	v1.resize(25, 3);
	cout << v1.size() << endl;
	cout << v1.capacity() << endl;

	v1.resize(5, 1);
	cout << v1.size() << endl;
	cout << v1.capacity() << endl;

}
void text_vector4()
{
	vector<int> v(10, 1);
	v.push_back(2);
	v.insert(v.begin(), 0);

	vector<int>:: iterator it = v.begin();
	while (it != v.end())
	{
		cout << *it << ' ';
		it++;
	}
	cout << endl;

	it = v.begin();
	v.insert(v.begin() + 5, 8);
	while (it != v.end())
	{
		cout << *it << ' ';
		it++;
	}
	cout << endl;
}
int main()
{
	text_vector4();
}