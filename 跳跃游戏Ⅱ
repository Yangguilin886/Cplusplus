class Solution {
public:
    int jump(vector<int>& nums) {
        int n=nums.size();
        vector<int> dp(n);
        for(int i=1;i<n;i++)
        {
            int min_i=10000;
            for(int j=0;j<i;j++)
            {
                if(j+nums[j]>=i)min_i=min(min_i,dp[j]);
            }
            dp[i]=min_i+1;
        }
        return dp[n-1];
    }
};