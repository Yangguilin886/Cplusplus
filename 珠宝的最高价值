class Solution {
public:
    int jewelleryValue(vector<vector<int>>& frame) {
        int dp[201][201]={0};
        for(int i=0;i<frame.size();i++)
        {
            for(int j=0;j<frame[0].size();j++)
            {
                int n=0,m=0;
                if(i-1>=0)n=dp[i-1][j];
                if(j-1>=0)m=dp[i][j-1];
                dp[i][j]=max(n,m)+frame[i][j];
            }
        }
        return dp[frame.size()-1][frame[0].size()-1];
    }
};