#include<iostream>
#include<string>
#include<algorithm>
using namespace std;
int n=0,len,a[100000]={0};
string d;
void Func()
{
    for(int i=1;i<=len;i++)
    {
        a[i]*=2;
    }
    for(int i=1;i<=len;i++)
    {
        a[i+1]+=a[i]/10;
        a[i]=a[i]%10;
    }
    if(a[len+1])
        len++;
}
int main()
{
    cin>>n>>d;
    reverse(d.begin(),d.end());
    int p=0;
    for(int i=0;i<d.size();i++)
    {
        if(d[i]=='.')
        {
            p=i;
            break;
        }
    }
    d.erase(d.begin()+p);
    len=d.size();
    for(int i=0;i<d.size();i++)
    {
        a[i+1]=d[i]-'0';
    }
    for(int i=1;i<=n;i++)
    {
        Func();
    }
    if(a[p]>=5)
    {
        a[p+1]++;
    }
    for(int i=p+1;i<=len;i++)
    {
        a[i+1]+=a[i]/10;
        a[i]=a[i]%10;
    }
    if(a[len+1])len++;
    for(int i=len;i>p;i--)
    {
        cout<<a[i];
    }
    return 0;
}